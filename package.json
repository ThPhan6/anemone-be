{
  "name": "anemone-api",
  "version": "0.0.1",
  "description": "Anemone API",
  "author": "",
  "private": true,
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:webstorm-bebug": "node --inspect-brk -r ts-node/register -r tsconfig-paths/register ./src/main.ts",
    "start:prod": "yarn migrate:up && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/`jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migrate:generate": "yarn typeorm -- migration:generate ./src/common/migrations/migration -d ./typeOrmConfig.ts",
    "migrate:up": "yarn typeorm -- migration:run -d ./typeOrmConfig.ts",
    "migrate:down": "yarn typeorm -- migration:revert -d ./typeOrmConfig.ts",
    "docker": "docker-compose up",
    "docker:app": "docker rm -f app & docker rmi -f base_app & docker-compose up base_app",
    "docker:db": "docker-compose up pg-write pg-read-1 pg-read-2",
    "docker:log": "docker logs -f -n 0 -- ",
    "docker:dev": "cd docker && docker-compose -f docker-compose.dev.yml up -d && cd ../",
    "prepare": "husky install",
    "seed": "ts-node -r tsconfig-paths/register ./src/common/seeders/main.seeder.ts"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.713.0",
    "@aws-sdk/client-iot": "^3.758.0",
    "@aws-sdk/client-s3": "^3.352.0",
    "@aws-sdk/credential-providers": "^3.370.0",
    "@aws-sdk/s3-request-presigner": "^3.363.0",
    "@aws-sdk/types": "^3.347.0",
    "@nestjs/common": "^11.0.12",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.12",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.12",
    "@nestjs/swagger": "^11.1.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/nestjs": "^9.9.0",
    "@sentry/profiling-node": "^9.9.0",
    "@types/async": "^3.2.20",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash": "^4.14.195",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.1.4",
    "exceljs": "^4.3.0",
    "firebase-admin": "^11.10.0",
    "forge": "^2.3.0",
    "iconv-lite": "^0.6.3",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "str-util": "^2.3.39",
    "ts-node": "^10.9.1",
    "ts-toolbelt": "^9.6.0",
    "tsconfig-paths": "^4.2.0",
    "twilio": "^4.13.0",
    "typeorm": "^0.3.21",
    "typeorm-extension": "^3.6.3",
    "wanakana": "^5.1.0",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^11.0.9",
    "@trivago/prettier-plugin-sort-imports": "^5.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/lodash": "^4.14.195",
    "@types/multer": "^1.4.7",
    "@types/node": "18.16.0",
    "@types/node-forge": "^1.3.11",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "29.4.3",
    "jest-cli": "29.4.3",
    "jest-config": "29.4.3",
    "lint-staged": "^13.2.2",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.82.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
